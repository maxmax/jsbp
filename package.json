{
  "name": "jsbp",
  "version": "0.1.0",
  "description": "JS Boilerplate / jsbp",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "client-start": "npm-run-all --parallel dev:server lint:watch",
    "dev:server": "webpack-dev-server --open --config webpack.dev.js",
    "watch": "webpack -w -d",
    "build": "webpack --config webpack.prod.js",
    "obfuscated": "webpack --config webpack.obfuscated.js",
    "deploy": "gh-pages -d dist",
    "lint": "node_modules/.bin/esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "start-electron": "electron main.js",
    "start-server": "NODE_PATH=./src nodemon server.js",
    "electron-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/app-icon/mac/app.icns --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maxmax/jsbp.git"
  },
  "engines": {
    "node": "8.11.3"
  },
  "author": "maxmax",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "8.6.4",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.6.0",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "csv-loader": "2.1.1",
    "electron": "2.0.8",
    "electron-installer-dmg": "1.0.0",
    "electron-packager": "12.1.0",
    "eslint": "4.7.0",
    "eslint-watch": "3.1.2",
    "express": "4.16.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "gh-pages": "1.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.5.3",
    "npm-run-all": "4.1.1",
    "postcss-loader": "2.1.5",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "uglifyjs-webpack-plugin": "0.4.6",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "xml-loader": "1.2.1"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "chalk": "2.4.1",
    "cross-fetch": "2.2.2",
    "etherscan-api": "8.0.4",
    "immutable": "3.8.2",
    "jsdom": "13.0.0",
    "lodash": "4.17.4",
    "mocha-jsdom": "2.0.0",
    "nodemon": "1.18.5",
    "prop-types": "15.6.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.3.0",
    "wasm-loader": "1.3.0",
    "window": "4.2.5"
  }
}
